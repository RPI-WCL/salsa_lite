module salsa_lite.local_fcs.util;

import object java.lang.System;

behavior Timer {

	long start_time, stop_time;

	Timer() {
	}

	long startTimer() {
		start_time = System.currentTimeMillis();
		pass start_time;
	}

	long stopTimer() {
		stop_time = System.currentTimeMillis();
		pass stop_time - start_time;
	}
}
